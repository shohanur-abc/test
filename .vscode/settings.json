{
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "todo-tree.general.tags": [
        "TODO",
        "FIXME",
        "HACK",
        "BUG",
        "PERF",
        "NOTE",
        "REVIEW",
        "WARN"
    ],
    "todo-tree.highlights.defaultHighlight": {
        "icon": "alert",
        "type": "text-and-comment",
        "foreground": "#fff",
        "background": "#e67d229f",
        "iconColour": "#e67e22"
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "check",
            "foreground": "#fff",
            "background": "#3498db8a",
            "iconColour": "#3498db"
        },
        "FIXME": {
            "icon": "flame",
            "foreground": "#fff",
            "background": "#e74d3cae",
            "iconColour": "#e74c3c"
        },
        "BUG": {
            "icon": "bug",
            "foreground": "#fff",
            "background": "#c03a2b96",
            "iconColour": "#c0392b"
        },
        "HACK": {
            "icon": "tools",
            "foreground": "#fff",
            "background": "#8d44ad96",
            "iconColour": "#8e44ad"
        },
        "PERF": {
            "icon": "rocket",
            "foreground": "#fff",
            "background": "#27ae5f88",
            "iconColour": "#27ae60"
        },
        "NOTE": {
            "icon": "info",
            "foreground": "#fff",
            "background": "#2ecc7077",
            "iconColour": "#2ecc71"
        },
        "REVIEW": {
            "icon": "eye",
            "foreground": "#fff",
            "background": "#f39d1291",
            "iconColour": "#f39c12"
        }
    },
    "todo-tree.general.statusBar": "tags",
    "tailwindCSS.experimental.classRegex": [
        [
            "cva\\(([^)]*)\\)",
            "'([^']*)'"
        ],
        [
            "cn\\(([^)]*)\\)",
            "'([^']*)'"
        ]
    ]
}